{
  "name": "examine-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "eslint 'src/**/*.{jsx,ts,tsx}' --fix",
    "prettier ": "prettier --write src",
    "typecheck": "tsc src/*.{ts,tsx} --noEmit --pretty",
    "test": "yarn lint && yarn typecheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "autoprefixer": "^10.4.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-sznm": "^1.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint-fix",
      "yarn prettier"
    ]
  }
}
